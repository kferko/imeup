<%= link_to "Logout", destroy_user_session_path, method: :delete %>
<% contacts = current_user.contacts_A + current_user.contacts_B %>
<% contacts.each_with_index do |contact, index| %>
    <tr>
      <td><%= index +=1 %></td>
      <td><%= contact.email %></td>
      <td>
        <%= link_to "Send Message", up_box_path(id: contact.id), remote: true, class: "btn btn-success btn-xs start-conversation" %>
      </td>
    </tr>
<% end %>
