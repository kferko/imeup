<div class="grid-stack top">
    <div class="grid-stack-item"
        data-gs-x="0" data-gs-y="0"
        data-gs-width="1" data-gs-height="1">
            <div class="grid-stack-item-content"></div>
    </div>
    <div class="grid-stack-item"
        data-gs-x="1" data-gs-y="0"
        data-gs-width="10" data-gs-height="1">
            <div class="grid-stack-item-content">
            	<div class="topicons">
					<div class="eye_top_1"></div>
					<a href="#" rel="profile"><div class="me_top_1"></div></a>
					<a href="#" rel="contacts"><div class="up_top_1"></div></a>
				</div>
            </div>
    </div>
    <div class="grid-stack-item"
        data-gs-x="11" data-gs-y="0"
        data-gs-width="1" data-gs-height="1">
            <div class="grid-stack-item-content"><div class="signout"><%= link_to "", destroy_user_session_path, method: :delete %></div></div>
    </div>
</div>

<div class="grid-stack main">
    <div class="grid-stack-item"
        data-gs-x="0" data-gs-y="0"
        data-gs-width="2" data-gs-height="6">
            <div class="grid-stack-item-content">
                <div class="sidebar">
                    <div id="contacts" class="sidedisplay">
                    	<% contacts = current_user.contacts_A + current_user.contacts_B %>
        				<% contacts.each_with_index do |contact, index| %>
        				    <tr>
        				    <br>
        				      <td><%= contact.email %></td>
        				      <td>
        				        <br><%= link_to "hello", up_box_path(id: contact.id), remote: true, class: "startchat" %><br>
        				      </td>
        				    </tr>
        				<% end %>
                    </div>
                    <div id="profile" class="sidedisplay"></div>
                </div>
            </div>
    </div>
    <div class="grid-stack-item firstbox"
        data-gs-x="2" data-gs-y="0"
        data-gs-width="3" data-gs-height="5">
            <div class="grid-stack-item-content"></div>
    </div>
</div>

<div class="grid-stack bott">
    <div class="grid-stack-item"
        data-gs-x="0" data-gs-y="0"
        data-gs-width="1" data-gs-height="1">
            <div class="grid-stack-item-content"><div class="ghost"></div></div>
    </div>
    <div class="grid-stack-item"
        data-gs-x="1" data-gs-y="0"
        data-gs-width="10" data-gs-height="1">
            <div class="grid-stack-item-content"></div>
    </div>
    <div class="grid-stack-item"
        data-gs-x="11" data-gs-y="0"
        data-gs-width="1" data-gs-height="1">
            <div class="grid-stack-item-content"><div class="legend"></div></div>
    </div>
</div>
