<%= link_to "Logout", destroy_user_session_path, method: :delete %>
<% current_user.contacts.each_with_index do |contact, index| %>
    <tr>
      <td><%= index +=1 %></td>
      <td><%= contact.email %></td>
      <td>
        <%= link_to "Send Message", "home#", class: "btn btn-success btn-xs start-conversation", "data-sid" => current_user.id, "data-rip" => contact.id %>
      </td>
    </tr>
<% end %>
